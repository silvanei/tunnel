{
    "name": "s3/tunnel",
    "version": "0.0.1",
    "description": "Expose localhost to internet",
    "authors": [
        {
            "name": "Silvanei Soares Santos",
            "email": "ads.silvanei@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "S3\\Tunnel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\S3\\Tunnel\\": "tests/"
        }
    },
    "require": {
        "php": "~8.3",
        "ext-swoole": "*",
        "ext-sodium": "*",
        "monolog/monolog": "^3.6",
        "guzzlehttp/guzzle": "^7.8",
        "nikic/fast-route": "^1.3",
        "laminas/laminas-stratigility": "^3.11",
        "laminas/laminas-diactoros": "^3.3",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "ext-inotify": "*",
        "swoole/ide-helper": "^5.1",
        "squizlabs/php_codesniffer": "^3.10",
        "phpstan/phpstan": "^1.11",
        "phpunit/phpunit": "^11.2",
        "infection/infection": "^0.29.5"
    },
    "scripts": {
        "check": [
            "@phpcs",
            "@phpstan",
            "@test"
        ],
        "phpcs": "phpcs",
        "phpstan": "phpstan analyse",
        "test": "XDEBUG_MODE=off phpunit",
        "test-coverage": "XDEBUG_MODE=coverage phpunit --coverage-html vendor/a_coverage/",
        "test-infection": "XDEBUG_MODE=coverage infection"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
